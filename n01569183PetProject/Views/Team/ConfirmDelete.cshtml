@model n01569183PetProject.Models.TeamDto
@{
    ViewBag.Title = "Delete";
    Layout = "";
}

<h2>Confirm Deletion</h2>
<p>Are you sure you want to delete the team: @Model.TeamName?</p>
<form method="post" action="/Team/Delete/@Model.TeamId">
    <input name="TeamId" value="@Model.TeamId" type="hidden" />
    <fieldset class="form-group">
        <button type="button" onclick="cancel('DeleteConfirmationBox')" class="btn btn-info">Cancel</button>
        <button type="submit" class="btn btn-danger">Delete</button>
    </fieldset>

</form>
